; Overridden States (with addded SFX)

;---------------------------------------------------------------------------
; Stand
[Statedef 0]
type = S
physics = S
sprpriority = 0

[State haha]
type = RemoveExplod
trigger1 = 1 && NumExplod(102401) = 1
id = 102401
ignorehitpause = 1

[State 0, 1]
type = ChangeAnim
trigger1 = Anim != 0 && Anim != 5
trigger2 = Anim = 5 && AnimTime = 0 ;Turn anim over
value = 0

[State 0, 2]
type = VelSet
trigger1 = Time = 0
y = 0

[State 0, 3] ;Stop moving if low velocity or 4 ticks pass
type = VelSet
trigger1 = Abs(Vel x) < 2
trigger2 = Time = 4
x = 0

;---------------------------------------------------------------------------
; Walk
[Statedef 20]
type    = S
physics = S
sprpriority = 0

[State 20, 1]
type = VelSet
trigger1 = Command = "holdfwd"
x = Const(velocity.walk.fwd.x)

[State 20, 2]
type = VelSet
trigger1 = Command = "holdback"
x = Const(velocity.walk.back.x)

[State 20, 3]
type = ChangeAnim
triggerall = Vel x > 0
trigger1 = Anim != 20 && Anim != 5 && Anim != 22
trigger2 = Anim = 5 && AnimTime = 0
value = 20 + (2 * (var(16) = 0))

[State 20, 4]
type = ChangeAnim
triggerall = Vel x < 0
trigger1 = Anim != 21 && Anim != 5 && Anim != 23
trigger2 = Anim = 5 && AnimTime = 0
value = 21 + (2 * (var(16) = 0))

; RUN_FWD
[Statedef 100]
type    = S
physics = S
facep2 = 0

; Nudge!
[State loser!]
type = ChangeState
triggerall = Time > 10
triggerall = P2BodyDist X < 20
trigger1 = Command = "fwd_x"
trigger2 = Command = "fwd_y" && (p2stateno != [5080, 5120])
trigger3 = Command = "fwd_z"
value = 205

[State loser!]
type = VarSet
triggerall = P2BodyDist X < 60
trigger1 = 1
var(21) = 1

[State 100, 2]
type = VelSet
trigger1 = AnimElemNo(0) >= 2
x = Const(velocity.run.fwd.x)

[State 100, 1]
type = ChangeAnim
trigger1 = Time = 0 && Anim != 100
value = 100

[State 100, 3]
type = PlaySnd
triggerall = var(24) = 0
trigger1 = Time = 0
value = S3, 10
channel = 9
loop = 1

[State 100, 1]
type = VarSet
triggerall = var(24) = 1
trigger1 = Time = 0
trigger2 = var(3) > 5
var(3) = 1

[State 100, 1]
