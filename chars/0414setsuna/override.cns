triggerall = var(24) = 1
trigger1 = AnimElem = 7,0 && Time != [0,10]
var(3) = 2

[State advanced 1]
type = VarAdd
triggerall = var(24) = 1
trigger1 = AnimElem = 7,0 && Time != [0,10]
var(4) = 2

[State 100, 8]
type = ChangeState
trigger1 = Time >= 14 && Command != "holdfwd" && !var(31) && Command != "holddown"
trigger2 = Time >= 14 && var(31) && P2BodyDist X <= 50 && P2MoveType = H
value = 101

