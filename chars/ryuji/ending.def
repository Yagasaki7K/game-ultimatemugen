; Ending cutscene for YAMAZAKI's KOF97 story.

; Like the intro, this storyboard has multiple scenes.
; Each scene uses two overlay anims, one for the picture, and
; another for the text.
; The fadein and fadeout parameters are used to fade one screen
; to the next.

[SceneDef]
spr = ending.sff
startscene = 0         ;Starting scene number (for debugging)

;------------------------------------
;START
[Scene 0]
; Fade
fadein.time = 200       ;Time to take for fading in
fadein.col = 100,-100,-100     ;Use 255,255,255 for white-fade
fadeout.time = 20
fadeout.col =200,-100,-100
; Screen color
clearcolor = 0,0,0
; Overlay Anims (from 0 to 9)
layerall.pos = 0,0   ;Default position for all layers
layer0.anim =  1
layer0.offset = 155,135
layer0.starttime = 0 
layer1.anim =  2
layer1.offset = 85,135
layer1.starttime = 50 
layer2.anim =  3
layer2.offset = 262,135
layer2.starttime = 50 
layer3.anim =  0       ;Anim action number
layer3.offset = 0,0    ;Position offset
layer3.starttime = 0   ;Time to start display
layer4.anim =  4       ;Anim action number
layer4.offset = 29,166 ;Position offset
layer4.starttime = 120 ;Time to start display

; Music
bgm = ending.mp3       ;Name of music file to play
bgm.loop = 0
; Total time
end.time = 900

