; Definition file for Kung Fu Man
; Contains all the filenames needed for the character

; Player information
[Info]
name = "Deadpool"        ;Name of character
displayname = "Deadpool" ;Name of character to display
versiondate = 07,08,2007    ;Version date of character (MM-DD-YYYY)
mugenversion = 04,14,2002   ;Version of M.U.G.E.N character works on
author = "Unlimited Team - sprites, Wucash - code"         ;Character author name
pal.defaults = 6,3,4,2      ;Default palettes in order of preference

; Files for the player
[Files]
cmd     = Deadpool.cmd   ;Command set
cns     = Deadpool.cns   ;Constants
st      = Deadpool.cns   ;States
stcommon = common1.cns ;Common states
sprite  = Deadpool.sff   ;Sprite
anim    = Deadpool.air   ;Animation
sound   = Deadpool.snd   ;Sound
pal1    = dead1.act  ;Palettes (can have up to 12)
pal2    = joepalette.act
pal3    = ultimatepalette.act
pal4    = xmenpalette.act
pal5    = fantomexpalette.act
pal6    = newxpalette.act
pal7    = xmaspalette.act

; Arcade mode
[Arcade]
intro.storyboard = intro.def
ending.storyboard = ending.def
