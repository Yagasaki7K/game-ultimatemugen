
[Info];
name = "The Grassy Plain-night-"

;----------------------------------------------------------------------
[Camera];
startx = -11 ;		一番最初のカメラ位置(X軸)
starty = 200 ;		一番最初のカメラ位置(Y軸)
boundleft = -115;		左画面端の距離
boundright = 76;		右画面端の距離
boundhigh = -120 ;		上にスクロールする距離。0 = スクロールせず　高いと上が見える
boundlow = 87;		下画面端の距離だが使用しない
verticalfollow = 1.02 ;	ジャンプした時、ステージ全体が上下する強さ　高いと少しジャンプするだけでスクロールする
tension = 40 ;		スクロールできる状態でそれ以上のドット数だけ画面端に行くと、スクロールする　低いと壁へ移動できない
floortension = 250
;-----------------------------------------

[PlayerInfo]
;--- Player 1 ---
p1startx = -75 ;		１Pの最初の立ち位置X軸
p1starty = 0 ;		１Pの最初の立ち位置Y軸
p1startz = 0 ;		１Pの最初の立ち位置Z軸だが使用しない
p1facing = 1 ;		１Pの最初の向き。1 = 左向き -1 = 右向き
;--- Player 2 ---
p2startx = 53
p2starty = 0
p2startz = 0
p2facing = -1
;--- Common --- ;		変更不要
leftbound = -1000 ;		Left bound (x-movement)
rightbound = 1000 ;		Right bound
topbound = 0 ;		Top bound (z-movement)
botbound = 0 ;		Bottom bound
;--------------------------------------------------------
[Scaling] ;			変更不要
topz = 0 ;			Top z-coordinate for scaling
botz = 50 ;		Bottom z-coordinate for scaling
topscale = 1 ;		Scale to use at top
botscale = 1.2 ;		Scale to use at bottom
;---------------------------------------------------------
[Bound]
screenleft = 18 ;		左画面端の壁の存在距離 boundleft を基準に見えない壁を作る　値が低いとキャラが画面外にめり込む
screenright = 18 ;		右画面端の壁の存在距離 boundright を基準に見えない壁を作る　値が低いとキャラが画面外にめり込む

;--------------------------------------------------------
[StageInfo]
zoffset = 293;		地面の位置	-y
autoturn = 1 ;		このステージでは、キャラが自動的に振り向くかどうか。1だと自動的に振り向く
resetBG = 0 ;		ラウンドが変わったら、全てのアニメをリセットするかどうか。1だとリセット
hires = 1
;--------------------------------------------------------

[Shadow]
color = 30,30,30
intensity = 25 ;		影の濃さ 0以下で非表示 256で黒 512で白 798で茶 1024は変な色
yscale = 0.27 ;		影の長さ 1が原寸大
fade.range = -300,-50 ;	不明 
;--------------------------------------------------------
[Reflection]
intensity = 0 ;		床に反射している色の濃さ 0以下で反射せず 256以下で大理石の床のような効果 256で原色と同じ 256以上で色化け
;--------------------------------------------------------
[Music]
;bgmusic = 
;bgvolume = 255 ;			ボリューム
