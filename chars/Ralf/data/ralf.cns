; コモンステート 
	
; 0: Stand 
[Statedef 0]
type = S
physics = S
sprpriority = 0
VelSet = 0,0

[State 0, ChangeAnim]
type = ChangeAnim
trigger1 = anim != 0 && anim != 5
trigger2 = anim = 5 && animtime = 0
value = var(0)
 
; 10: Stand to Crouch 
[Statedef 10]
type    = C
physics = C
anim = 10
VelSet = 0,0

[State 10, ChangeAnim]
type = ChangeState
trigger1 = animtime = 0
value = 11
 
; 20: Walk 
[Statedef 20]
type    = S
physics = S
sprpriority = 0

[State 20, VelSet]
type = VelSet
trigger1 = command = "holdfwd"
x = const(velocity.walk.fwd.x)

[State 20, VelSet]
type = VelSet
trigger1 = command = "holdback"
x = const(velocity.walk.back.x)

[State 20, VelMul]
type = VelMul
trigger1 = var(51) = 1
x = 1.3

[State 20, ChangeAnim]
type = ChangeAnim
trigger1 = vel x &&  anim != [20,21]
value = 20 + (vel x < 0)
 
; 40-52: Jump 
	
; 40: Jump Start 
[Statedef 40]
type    = S
physics = S
anim = 40
ctrl = 0
sprpriority = 1
VelSet = 0,0

[State 40, VarSet]
type = VarSet
trigger1 = time = 0
sysvar(1) = ifelse(command = "holdfwd",1,ifelse(command = "holdback", -1, 0))

[State 40, VarSet]
type = VarSet
trigger1 = time = 0
var(1) = ifelse(command = "highjump" || prevstateno = 101 || prevstateno = 116, 1, 0)

[State 40, VarSet]
type = VarSet
trigger1 = command != "holdup"
var(1) = var(1) | 2

; 通常/小ジャンプのvel x
[State 40, VelSet]
type = VelSet
trigger1 = animtime = 0
x = ifelse(sysvar(1)=0,0,ifelse(sysvar(1)=1, const(velocity.jump.fwd.x), const(velocity.jump.back.x)))

; 中/大ジャンプのvel x
[State 40, VelSet]
type = VelSet
trigger1 = animtime = 0 && (var(1) & 1)
x = ifelse(sysvar(1)=0,0,ifelse(sysvar(1)=1,4.5,-4.5))

; 通常/大ジャンプのvel y
[State 40, VelSet]
type = VelSet
trigger1 = animtime = 0
y = const(velocity.jump.y)

; 小/中ジャンプのvel y
[State 40, VelSet]
type = VelSet
trigger1 = animtime = 0 && (var(1) & 2)
y = -6

[State 40, AfterImage]
type = AfterImage
trigger1 = animtime = 0
trigger1 = vel x && (var(1) & 1)
time = 12
length = 7
framegap = 3
trans = add1
palcontrast = 128,128,128
paladd = 0,0,0
palmul = 1,1,1

[State 40, ChangeState]
type = ChangeState
trigger1 = animtime = 0
value = 50
ctrl = 1
 
; 50: Jump Up 
[Statedef 50]
type    = A
physics = A

[State 50, VarSet]
type = VarSet
trigger1 = time = 0
sysvar(1) = 0

[State 50, ChangeAnim]
type = ChangeAnim
trigger1 = time = 0
value = ifelse((vel x)=0, 41, ifelse((vel x)>0, 42, 43)) + (var(1) & 2) * 5

[State 50, PlaySnd]
type = PlaySnd
trigger1 = time = 1
value = 0,ifelse((var(1) & 1) && vel x,2,1)
 
