
[Info];
name = "The Central Garden of Tohno's Mansion"

;----------------------------------------------------------------------
[Camera];
startx = 0 ;		一番最初のカメラ位置(X軸)
starty = 0 ;		一番最初のカメラ位置(Y軸)
boundleft = -140;		左画面端の距離
boundright = 90;		右画面端の距離
boundhigh = -190 ;		上にスクロールする距離。0 = スクロールせず　高いと上が見える
boundlow = 20 ;		下画面端の距離だが使用しない
verticalfollow = .9845 ;	ジャンプした時、ステージ全体が上下する強さ　高いと少しジャンプするだけでスクロールする
tension = 50 ;		スクロールできる状態でそれ以上のドット数だけ画面端に行くと、スクロールする　低いと壁へ移動できない
floortension = 132
;-----------------------------------------

[PlayerInfo]
;--- Player 1 ---
p1startx = -80 ;		１Pの最初の立ち位置X軸
p1starty = 0 ;		１Pの最初の立ち位置Y軸
p1startz = 0 ;		１Pの最初の立ち位置Z軸だが使用しない
p1facing = 1 ;		１Pの最初の向き。1 = 左向き -1 = 右向き
;--- Player 2 ---
p2startx = 80
p2starty = 0
p2startz = 0
p2facing = -1
;--- Common --- ;		変更不要
leftbound = -1000 ;		Left bound (x-movement)
rightbound = 1000 ;		Right bound
topbound = 0 ;		Top bound (z-movement)
botbound = 0 ;		Bottom bound
;--------------------------------------------------------
[Scaling] ;			変更不要
topz = 0 ;			Top z-coordinate for scaling
botz = 50 ;		Bottom z-coordinate for scaling
topscale = 1 ;		Scale to use at top
botscale = 1.2 ;		Scale to use at bottom
;---------------------------------------------------------
[Bound]
screenleft = 18 ;		左画面端の壁の存在距離 boundleft を基準に見えない壁を作る　値が低いとキャラが画面外にめり込む
screenright = 18 ;		右画面端の壁の存在距離 boundright を基準に見えない壁を作る　値が低いとキャラが画面外にめり込む

;--------------------------------------------------------
[StageInfo]
zoffset = 240;		地面の位置	-y
autoturn = 1 ;		このステージでは、キャラが自動的に振り向くかどうか。1だと自動的に振り向く
resetBG = 0 ;		ラウンドが変わったら、全てのアニメをリセットするかどうか。1だとリセット


Hires = 1
;D4・D0mugen専用記述 D4mugenにて不具合が出る場合はこの記述を消す
;"Hires = 1" is for only "D4&D0(Hires&nomal)mugen".
;If some problem happen on "D4(Hires)mugen",please delete this line"Hires = 1"
 
;--------------------------------------------------------

[Shadow]
intensity = 128 ;		影の濃さ 0以下で非表示 256で黒 512で白 798で茶 1024は変な色
yscale = 0.35 ;		影の長さ 1が原寸大
fade.range = -300,-50 ;	不明 
;--------------------------------------------------------
[Reflection]
intensity = 0 ;		床に反射している色の濃さ 0以下で反射せず 256以下で大理石の床のような効果 256で原色と同じ 256以上で色化け
;--------------------------------------------------------
[Music]
;bgmusic = sound/ ;	BGM
;bgvolume = 255 ;			ボリューム
;-------------------------------------
[BGdef]
spr = stages/AC_miyako.sff ;	このステージに使う、SFFの位置指定
debugbg = 0 ;		1にすると、画像が無い部分をマゼンタで塗りつぶす デバッグ後は0に

[BG 000]
type = normal ;		一枚絵の場合は、これで指定
spriteno = 0,6 ;		sffに登録してある番号
layerno = 0 ;		0なので、キャラよりも奥に表示される 1で手前に表示
start = -600,-380 ;		画面に対する、この画像の位置	+x,-y
delta = 1.58,1.58 ;		画面が動いたときの移動量	小数可	971mugenの場合は2で不動　2以下で早く動き（奥、手前）、2以上で遅く動く（奥）
trans = none ;		登録した画像で表示(None)　他に、add,subがある
mask = 0;		透過色を透過するかどうか。1で透過 0で透過せず

